{"ast":null,"code":"import data from './assets/movies'; // 영화 데이터\nimport Navbar from './components/Navbar.vue';\nimport Event from './components/Event.vue'; // 이벤트 박스\nimport Modal from './components/Modal.vue';\nconsole.log(data);\nexport default {\n  name: 'App',\n  // 문자의 표시될 변수 표기기\n  data() {\n    return {\n      isModal: false,\n      data: data,\n      selectedMovie: 0,\n      title: \"NEPLIX SQUID GAME\"\n    };\n  },\n  methods: {\n    increseLike(i) {\n      this.data[i].like += 1;\n    }\n  },\n  components: {\n    Navbar: Navbar,\n    Event: Event,\n    Modal: Modal\n  }\n};","map":{"version":3,"names":["data","Navbar","Event","Modal","console","log","name","isModal","selectedMovie","title","methods","increseLike","i","like","components"],"sources":["C:\\vue project\\movie-info\\src\\App.vue"],"sourcesContent":["<template>\n  <Navbar />\n  <Event :text=\"title\" /> \n  <h1>영화정보</h1> \n  <div v-for=\"(movie, i) in data\" :key=\"i\" class=\"item\">\n    <figure>\n      <img :src=\"`${movie.imgUrl}`\" :alt=\"movie.title\">\n    </figure>\n    <div class=\"info\">\n      <h3>{{movie.title}}</h3>\n      <p>개봉 : {{movie.year}}</p>\n      <p>장르 : {{movie.category}}</p>\n      <button @:click=\"increseLike(i)\">좋아요\n      </button> \n      <span>{{movie.like}}</span>\n      <p>\n        <button @:click=\"isModal=true; selectedMovie=i\">상세보기</button>\n      </p>\n    </div>\n  </div>\n  <Modal \n  :data=\"data\" \n  :isModal=\"isModal\" \n  :selectedMovie=\"selectedMovie\"/>\n\n</template>\n\n<script>\nimport data from './assets/movies'; // 영화 데이터\nimport Navbar from './components/Navbar.vue';\nimport Event from './components/Event.vue'; // 이벤트 박스\nimport Modal from './components/Modal.vue';\nconsole.log(data);\n  export default{\n    name: 'App',\n\n    // 문자의 표시될 변수 표기기\n    data(){\n      return {\n        isModal: false,\n        data: data,\n        selectedMovie: 0,\n        title: \"NEPLIX SQUID GAME\"\n      }\n    },\n    methods: {\n      increseLike(i){\n        this.data[i].like += 1;\n      },\n    },\n    components: {\n        Navbar: Navbar,\n        Event: Event,\n        Modal: Modal,\n      }\n  }\n</script>\n\n<style>\n*{\n  box-sizing: border-box;\n  margin: 0;\n}\n\n  body{\n    max-width: 768px;\n    margin: 0 auto;\n    padding: 20px;\n  }\n\n  h1, h2, h3\n  {\n    margin-bottom: 1rem;\n  }\n\n  p {\n    margin-bottom: 0.5rem;\n  }\n\n  button {\n    margin-right: 10px;\n    margin-top: 1rem;\n  }\n\n  .item{\n    width: 100%;\n    border: 1px solid #acc;\n    display: flex;\n    margin-bottom: 20px;\n    padding: 1rem;\n  }\n\n  .item figure{\n    width: 30%;\n    margin-right: 1rem;\n  }\n\n  .item img{\n    width: 100%;\n  }\n\n  .item .info{\n    width: 100%;\n  }\n\n  .modal{\n    background: rgba(0, 0, 0, 0.7);\n    position: fixed;\n    left: 0;\n    top: 0;\n    width: 100%;\n    height: 100vh;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n  }\n\n  .modal .inner{\n    background: #fff;\n    width: 80%;\n    padding: 20px;\n    border-radius: 10px;\n  }\n</style>\n"],"mappings":"AA4BA,OAAOA,IAAG,MAAO,iBAAiB,EAAE;AACpC,OAAOC,MAAK,MAAO,yBAAyB;AAC5C,OAAOC,KAAI,MAAO,wBAAwB,EAAE;AAC5C,OAAOC,KAAI,MAAO,wBAAwB;AAC1CC,OAAO,CAACC,GAAG,CAACL,IAAI,CAAC;AACf,eAAc;EACZM,IAAI,EAAE,KAAK;EAEX;EACAN,IAAIA,CAAA,EAAE;IACJ,OAAO;MACLO,OAAO,EAAE,KAAK;MACdP,IAAI,EAAEA,IAAI;MACVQ,aAAa,EAAE,CAAC;MAChBC,KAAK,EAAE;IACT;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,WAAWA,CAACC,CAAC,EAAC;MACZ,IAAI,CAACZ,IAAI,CAACY,CAAC,CAAC,CAACC,IAAG,IAAK,CAAC;IACxB;EACF,CAAC;EACDC,UAAU,EAAE;IACRb,MAAM,EAAEA,MAAM;IACdC,KAAK,EAAEA,KAAK;IACZC,KAAK,EAAEA;EACT;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}